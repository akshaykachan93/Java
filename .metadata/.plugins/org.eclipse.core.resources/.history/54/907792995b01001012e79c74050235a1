package com.TypesofVariales;

public class InstanceVariableORGlobalVariable {
		
		//int a=500;
		//int a;
		//String a;
		//boolean a;
		double a;
		int b;

		public static void main(String[] args) {
			
			

			//System.out.println(a); // it will not work directly
			
			InstanceVariableORGlobalVariable i= new InstanceVariableORGlobalVariable();
			
			System.out.println(i.a); //ObjectName.variableName
			System.out.println(i.b);

		}

	}